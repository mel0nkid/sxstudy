            <dependency>
                <groupId>com.melon.sx</groupId>
                <artifactId>sx-study-util</artifactId>
                <version>0.0.1-SNAPSHOT</version>
            </dependency>

            <dependency>
                <groupId>com.melon.sx</groupId>
